adio value={2}>Нет</Radio>\r\n            </Radio.Group>\r\n          </div>\r\n\r\n          <div className=\"buttons\">\r\n            <button className=\"btn\">Отмена</button>\r\n            <button className=\"btn\" onClick={onFileUpload}>\r\n              Отправить\r\n            </button>\r\n          </div>\r\n        </div>\r\n\r\n        <table>\r\n          <tr>\r\n            <td>Название</td>\r\n            <td>Последнее изменение</td>\r\n            <td>Размер</td>\r\n            <td>Тип</td>\r\n          </tr>\r\n\r\n          {filesArray.length\r\n            ? filesArray.map((element) => (\r\n                <tr key={element.name}>\r\n                  <td>{element.name}</td>\r\n                  <td>\r\n                    {moment(element.lastModifiedDate).format(\r\n                      'DD.MM.YYYY, hh:mm:ss'\r\n                    )}\r\n                  </td>\r\n\r\n                  <td>\r\n                    {Math.ceil((element.size / 1024) * 100) / 100} Кбайт\r\n                  </td>\r\n                  <td>\r\n                    документ {selectedFile ? transformType(element) : null}\r\n                  </td>\r\n                </tr>\r\n              ))\r\n            : null}\r\n        </table>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default TaskCreate;\r\n","C:\\Users\\Sergey\\Documents\\Пример\\code\\client\\src\\components\\Tasks\\TaskPage\\index.js",["215","216","217"],"import React, { useState, useEffect } from 'react';\r\nimport { useSelector, useDispatch } from 'react-redux';\r\nimport { Tabs } from 'antd';\r\n\r\nimport { TaskCreate } from 'components';\r\nimport { fetchAllDocuments } from 'redux/actions/documentAction';\r\n\r\nimport './index.scss';\r\nimport TableTasks from '../TableTasks';\r\n\r\nconst TaskPage = () => {\r\n  const [filesArray, setFilesArray] = useState([]);\r\n\r\n  const { token, documents, auth } = useSelector((state) => state);\r\n  const dispatch = useDispatch();\r\n\r\n  useEffect(()